$TTL	10 ; 
; $TTL used for all RRs without explicit TTL value
$ORIGIN @DOMAIN@.
@  1D  IN  SOA ns.@DOMAIN@. hostmaster.@DOMAIN@. (
			      2002022401 ; serial
			      10 ; refresh
			      15 ; retry
			      10 ; expire
			      10 ; minimum
			     )

       IN  NS     ns.@DOMAIN@. 

ns    IN A      @IP_ADDR@   
